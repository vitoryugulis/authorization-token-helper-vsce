{
	"version": "0.2.0",
    "configurations": [
        {
            "env": {
                "dev-endpoint":"http://pt-api-dev.guideinvestimentos.com.br/auth/users/login",
                "qa-endpoint":"http://pt-api-qa.guideinvestimentos.com.br/auth/users/login",
                "login": "admin_guide",
                "password": "Toor@2017"
            },
            "name": "Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm: watch"
        },
        {
            "name": "Extension Tests",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}",
                "--extensionTestsPath=${workspaceFolder}/out/test"
            ],
            "outFiles": [
                "${workspaceFolder}/out/test/**/*.js"
            ],
            "preLaunchTask": "npm: watch"
        }
    ]
}
